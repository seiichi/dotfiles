#!/bin/bash

#https://github.com/ChrisJohnsen/tmux-MacOSX-pasteboard/issues/://github.com/ChrisJohnsen/tmux-MacOSX-pasteboard/issues/8

if [ -x /usr/local/bin/reattach-to-user-namespace ] ; then
  /usr/local/bin/reattach-to-user-namespace $@
elif [ -x /usr/bin/reattach-to-user-namespace ] ; then
  /usr/bin/reattach-to-user-namespace $@
else
  exec "$@"
fi
